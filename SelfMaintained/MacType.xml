<?xml version="1.0"?>
<SettingsLocationTemplate xmlns="http://schemas.microsoft.com/UserExperienceVirtualization/2013A/SettingsLocationTemplate">
  <Name>MacType</Name>
  <ID>MacType</ID>
  <Version>1</Version>
  <Author>
    <Name>James Swineson</Name>
    <Email>github@public.swineson.me</Email>
  </Author>
  <Processes>
    <Process>
      <Filename>MacLoader.exe</Filename>
    </Process>
    <Process>
      <Filename>MacLoader64.exe</Filename>
    </Process>
    <Process>
      <Filename>MacTray.exe</Filename>
    </Process>
    <Process>
      <Filename>MacTuner.exe</Filename>
