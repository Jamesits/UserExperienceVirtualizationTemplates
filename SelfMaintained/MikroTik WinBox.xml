<?xml version="1.0"?>
<SettingsLocationTemplate xmlns="http://schemas.microsoft.com/UserExperienceVirtualization/2013A/SettingsLocationTemplate">
  <Name>MikroTik WinBox</Name>
  <ID>MikroTik-WinBox</ID>
  <Version>1</Version>
  <Author>
    <Name>James Swineson</Name>
    <Email>github@public.swineson.me</Email>
  </Author>
  <Processes>
    <Process>
      <Filename>winbox.exe</Filename>
    </Process>
  </Processes>
  <Settings>
    <Asynchronous>false</Asynchronous>
    <PreventOverlappingSynchronization>true</PreventOverlappingSynchronization>
    <File>
      <Root>
        <EnvironmentVariable>APPDATA</EnvironmentVariable>
      </Root>
      <Path Recursive="true">Mikrotik\Winbox</Path>
    </File>
  </Settings>
</SettingsLocationTemplate>